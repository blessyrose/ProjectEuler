public class smallestmultiple {
     public static void main(String[] args) {
         int x = 2520; 
         int multiple = 0; 
         for (int i = 11; i <= 20; i++) {
             if (x % i == 0) {
                 multiple = x; 
                 System.out.println(multiple);
                 break;
             } else {
                 x++;
             }
         }
     }
}
